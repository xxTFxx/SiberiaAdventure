{
    "multipart": [
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "iron_bars_post" }
        },
        {   "when": { "north": true },
            "apply": { "model": "ffff:barbedwirefence_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "ffff:barbedwirefence_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "ffff:barbedwirefence_side", "y": 180  }
        },
        {   "when": { "west": true },
            "apply": { "model": "ffff:barbedwirefence_side", "y": 270 }
        }
    ]
}
